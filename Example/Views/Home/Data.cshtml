@model Example.ViewModel.DataViewModel

<br />
<table class="table table-hover">
    <thead class="thead-light">
        <tr>
            <th>Original URL</th>
            <th>Short URL</th>
        </tr>
    </thead>
    @foreach (URLShort p in Model.UrlData)
    {
        <tr>
            <td><a class="navbar-brand" href="@p.LongURL">@p.LongURL</a></td>
            <td><a class="navbar-brand" href="@p.ShortURL">@p.ShortURL</a></td>
        </tr>
    }
</table>


